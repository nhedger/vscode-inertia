{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"lib": ["ESNext"],
		"moduleResolution": "Bundler",
		"esModuleInterop": true,
		"strict": true,
		"strictNullChecks": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"skipDefaultLibCheck": true,
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
